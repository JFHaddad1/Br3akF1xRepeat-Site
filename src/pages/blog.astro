---
import BaseLayout from '../layouts/BaseLayout.astro';
import BlogPost from '../components/BlogPost.astro';
const allPosts = Object.values(import.meta.glob('./posts/*.md', { eager: true }));
const siteTitle = "// Br3akF1xRepeat";
const pageTitle = "// Blog";
---
<BaseLayout siteTitle ={siteTitle} pageTitle={pageTitle}>
	<h1>Portfolio: Joseph Haddad</h1>
	<h2>This is my new Blog page</h2>
	<p>I am working through Astro's introductory tutorial. This is the third page on my website.</p>
	<p>This is where I will post about my journey learning Astro</p>
	<ul>
		{allPosts.map((post: any) => <BlogPost url={post.url} title={post.frontmatter.title} />)}
	</ul>
</BaseLayout>